unit Order_class;

interface

  type TOrder = Class
  private
    id : integer;
  public
    constructor Create(p_id : integer);
    procedure Edit(P_id: integer);
    destructor Destroy;
    function get_id : integer;

  End;
implementation

function TOrder.get_id;
begin
   get_id  := id;
end;

constructor TOrder.Create(p_id: Integer);
begin
  id := p_id;
end;

procedure TOrder.Edit(P_id: Integer);
begin
  id := p_id;
end;

destructor TOrder.Destroy;
begin

end;


end.
